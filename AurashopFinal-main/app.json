{
  "expo": {
    "name": "PANDORA SHOP",
    "slug": "PANDORA SHOP",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "https://1000logos.net/wp-content/uploads/2021/11/Pandora-Logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "https://1000logos.net/wp-content/uploads/2021/11/Pandora-Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.itgenius.aurashop",
      "supportsTablet": true,
      "userInterfaceStyle": "automatic",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to let you upload profile photos.",
        "NSPhotoLibraryUsageDescription": "This app uses the photo library to let you upload profile photos."
      }
    },
    "android": {
      "package": "com.itgenius.aurashop",
      "versionCode": 1,
      "ndkVersion": "26.0.10792818",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "The app accesses your camera to let you share photos with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "01eb2b8d-8ad5-4c5d-b67f-fe5872c5e19e"
      }
    },
    "owner": "suthamas"
  }
}
